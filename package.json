{
  "name": "react-streetlife",
  "description": "Newsfeed single page app for a coding challenge built using react, css-modules",
  "author": "Simon Nonnis (https://github.com/SimoNonnis)",
  "version": "0.0.5",
  "license": "MIT",
  "main": "src/main.js",
  "scripts": {
    "start": "webpack-dev-server --inline --hot",
    "clean": "rm -rf ./dist/*",
    "prebuild": "mkdir -p ./dist && npm run clean && cp favicon.ico ./dist",
    "build": "webpack -p --config webpack.config.prod.js",
    "postbuild": "http-server ./dist -p 8081 -d false --cors -o -c-1",
    "stats": "webpack --profile --json > stats.json",
    "test": "NODE_ENV=test mocha --require './src/test/setup.js' --recursive './src/test/*.test.js' --compilers js:babel-core/register --require ignore-styles"
  },
  "keywords": [
    "react",
    "reactjs",
    "webpack",
    "css-modules",
    "babel",
    "postcss",
    "postcss-cssnext",
    "web-font-loader",
    "hot-module-replacement",
    "eslint",
    "eslint-loader",
    "stylelint",
    "html-webpack-plugin",
    "http-server"
  ],
  "homepage": "https://github.com/SimoNonnis/react-streetlife#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/SimoNonnis/react-streetlife"
  },
  "bugs": {
    "url": "https://github.com/SimoNonnis/react-streetlife/issues"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^2.11.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.21.0",
    "http-server": "^0.9.0",
    "ignore-styles": "^4.0.0",
    "image-webpack-loader": "^1.8.0",
    "jsdom": "^9.4.1",
    "mocha": "^3.0.0",
    "normalize.css": "^4.2.0",
    "postcss-cssnext": "^2.6.0",
    "postcss-easy-import": "^1.0.1",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.3.3",
    "react-addons-css-transition-group": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "style-loader": "^0.13.1",
    "stylelint-webpack-plugin": "^0.2.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-validator": "^2.1.1"
  }
}
